{% extends 'base.html.twig' %}

{% block body %}
    <h1>Equipementoptionels list</h1>

    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Tachygraphe</th>
                <th>Plombagetachygraphe</th>
                <th>Fonctrefegirateur</th>
