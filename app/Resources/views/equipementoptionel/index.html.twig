                <td>{{ equipementoptionel.fontlecteur }}</td>
                <td>{{ equipementoptionel.fonctecrans }}</td>
                <td>{{ equipementoptionel.fonctmicrophone }}</td>
                <td>{{ equipementoptionel.rmqtachygraphe }}</td>
                <td>{{ equipementoptionel.rmqplombagetachygraphe }}</td>
                <td>{{ equipementoptionel.rmqfonctrefegirateur }}</td>
                <td>{{ equipementoptionel.rmqcamerasurvaillence }}</td>
                <td>{{ equipementoptionel.rmqafficheursdestination }}</td>
                <td>{{ equipementoptionel.rmqfonctradio }}</td>
                <td>{{ equipementoptionel.rmqfontlecteur }}</td>
                <td>{{ equipementoptionel.rmqfonctecrans }}</td>
                <td>{{ equipementoptionel.rmqfonctmicrophone }}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('equipementoptionel_show', { 'id': equipementoptionel.id }) }}">show</a>
