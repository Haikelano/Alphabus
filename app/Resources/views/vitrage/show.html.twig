        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('vitrage_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('vitrage_edit', { 'id': vitrage.id }) }}">Edit</a>
