{% extends 'base.html.twig' %}

{% block body %}
    <h1>Vitrage</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ vitrage.id }}</td>
            </tr>
            <tr>
                <th>Etat</th>
                <td>{{ vitrage.etat }}</td>
            </tr>
            <tr>
                <th>Overclose</th>
